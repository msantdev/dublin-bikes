{
     "env": {
       "node": true,
       "es2021": true
     },
     "extends": [
          "eslint:recommended",
          "plugin:@typescript-eslint/recommended",
          "plugin:prettier/recommended"
        ],
     "plugins": ["@typescript-eslint", "prettier"],
     "parserOptions": {
     "ecmaVersion": 12,
     "sourceType": "module"
     },
     "rules": {
       "no-unused-vars": "warn",
       "no-console": "warn",
       "eqeqeq": "error",
       "semi": ["error", "always"],
       "prettier/prettier": "error"
     }
}
   